CREATE OR REPLACE VIEW officedetails AS
SELECT officemaster.id, officename.shortOfficeName, office_address.Address 
FROM officemaster, officename, office_address
WHERE officemaster.officeName = officename.id
AND officemaster.officeAddress = office_address.placeId
ORDER BY 2, 3




--

